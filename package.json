    "babel-preset-stage-2": "^6.3.13",
    "babel-register": "^6.4.3",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^3.4.1",
    "esdoc": "^0.4.3",
    "eslint": "^2.1.0",
    "eslint-plugin-react": "^4.0.0",
    "express": "^4.13.3",
    "gh-release": "^2.0.2",
    "gobble": "^0.10.2",
    "gobble-babel": "^6.0.0",
    "gobble-cli": "^0.6.0",
    "gobble-eslint": "^0.1.0",
