  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/freemountain/fux.git"
  },
  "release-script": {
    "altPkgRootFolder": "dist/pkg"
  },
  "keywords": [
    "fux",
    "deku",
