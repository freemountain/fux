    "@f/zip-obj": "^1.1.1",
    "deku": "^2.0.0-rc16",
    "deku-memoize": "^1.2.0",
    "flyd": "^0.2.1",
    "grizzly": "^1.0.16",
    "immpatch": "^0.2.0",
    "immutable": "^3.7.6",
    "inflection": "^1.8.0",
